{
  "name": "betterfly-app",
  "version": "1.3.0",
  "main": "expo-router/entry",
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "build:development": "rm -rf dist/ && NODE_ENV=development eas build --local --platform android --profile development",
    "build:preview": "rm -rf dist/ && NODE_ENV=development eas build --local --platform android --profile preview",
    "build:production": "rm -rf dist/ && NODE_ENV=production eas build --local --platform android --profile production"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@tanstack/react-query": "^5.17.15",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "expo": "~50.0.5",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-keep-awake": "~12.8.2",
    "expo-linking": "~6.2.2",
    "expo-notifications": "~0.27.6",
    "expo-router": "~3.4.6",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.1.3"
  },
  "private": true
}
